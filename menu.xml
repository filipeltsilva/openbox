			<menu id="menu-arquivos-configuracao-openbox" label="Arquivos de Configuração">
				<item label="Autostart">
					<action name="Execute">
						<execute>subl ~/.config/openbox/autostart</execute>
					</action>
				</item>
				<item label="Environment">
					<action name="Execute">
						<execute>subl ~/.config/openbox/environment</execute>
